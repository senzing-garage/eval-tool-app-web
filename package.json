{
  "name": "senzing-eval-tool-app-web",
  "displayName": "Senzing Eval Tool (Web)",
  "readme": "README.md",
  "version": "0.0.1",
  "publisher": "Senzing",
  "license": "Apache-2.0",
  "author": {
    "name": "Senzing",
    "url": "https://senzing.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve @senzing/eval-tool-app-web",
    "start:clean": "rm -rf node_modules && npm i && npm start",
    "start:config": "node ./run/scripts/start-configserver -- grpcConnection=\"http://localhost:8260/grpc\" datamartServerUrl=\"http://localhost:8250\" webServerUrl=\"http://localhost:4200\" webServerCspDefaultSrc=\"http://localhost:4200\" confServerPortNumber=4201",
    "start:docker": "node ./run/scripts/start-webserver -- grpcConnection=\"http://localhost:8260/grpc\" datamartServerUrl=\"http://localhost:8250\"",
    "first-run": "npm install && npm run build:client:rest && npm run build:sdk && npm run build",
    "_postinstall": "npm run build:libs",
    "build": "ng build @senzing/eval-tool-app-web",
    "build:client:rest": "ng build @senzing/rest-api-client-ng",
    "build:docker": "ng build --configuration production",
    "build:libs": "npm run build:subrepos",
    "build:ui-common": "ng build @senzing/eval-tool-ui-common",
    "build:app": "npm run build:libs && ng build @senzing/eval-tool-app-web",
    "build:subrepos": "ng build @senzing/rest-api-client-ng && ng build @senzing/eval-tool-ui-common",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.11",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fontsource/material-icons": "^4.2.4",
    "@fontsource/roboto": "^4.2.2",
    "@playwright/test": "1.56.1",
    "@senzing/sz-sdk-typescript-grpc-web": "./packages/@senzing/sz-sdk-typescript-grpc-web/senzing-sz-sdk-typescript-grpc-web-4.0.0-beta.4.1.tgz",
    "@senzing/sz-sdk-typescript-grpc": "4.0.0-beta.4.0",
    "cors": "^2.8.5",
    "d3": "^7.8.4",
    "detect-file-encoding-and-language": "^2.4.0",
    "express": "^5.1.0",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^8.5.1",
    "helmet-csp": "^4.0.0",
    "http-proxy-middleware": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "ngx-json-viewer": "^3.2.1",
    "ngx-webstorage-service": "^5.0.0",
    "pug": "^3.0.0",
    "rxjs": "~7.8.0",
    "sanitize-filename": "^1.6.3",
    "sass": "^1.77.6",
    "serve-static": "^2.2.0",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular-devkit/schematics": "^19.2.12",
    "@angular-eslint/builder": "^19.2.0",
    "@angular-eslint/eslint-plugin": "^19.2.0",
    "@angular-eslint/eslint-plugin-template": "^19.2.0",
    "@angular-eslint/schematics": "^19.2.0",
    "@angular-eslint/template-parser": "^19.2.0",
    "@angular/cli": "^19.2.10",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/language-service": "^19.2.11",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~5.1.0",
    "concat": "^1.0.3",
    "concurrently": "^9.2.0",
    "eslint": "^9.34.0",
    "fs-extra": "^11.3.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.2",
    "playwright-ng-schematics": "^2.0.0",
    "rimraf": "^6.0.1",
    "scss-bundle": "^3.1.2",
    "typescript": "~5.7.2"
  }
